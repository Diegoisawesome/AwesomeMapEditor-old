                                                        <xctk:IntegerUpDown x:Name="iudNPCNumber"  MaxWidth="41" FontFamily="Courier New" Padding="0,2,0,0" DockPanel.Dock="Right" Value="0" FormatString="X2" Maximum="0xFF" Minimum="0" BorderThickness="0,1,1,1" ClipValueToMinMax="True"/>
                                                    </DockPanel>
                                                </Border>
                                            </Grid>
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="Auto" />
