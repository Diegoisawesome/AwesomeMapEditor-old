                                                    <DockPanel>
                                                        <TextBox DockPanel.Dock="Left" BorderThickness="1,1,0,1" Text="{Binding Source={x:Static bknd:Config.settings}, Path=HexPrefix}" IsReadOnly="True" IsTabStop="False" MaxLength="2" Background="{DynamicResource {x:Static SystemColors.InactiveSelectionHighlightBrushKey}}"/>
                                                        <xctk:IntegerUpDown x:Name="iudNPCSpriteNumber"  MaxWidth="41" FontFamily="Courier New" Padding="0,2,0,0" DockPanel.Dock="Right" Value="0" Maximum="0xFF" Minimum="0" FormatString="X2" BorderThickness="0,1,1,1" ClipValueToMinMax="True" DisplayDefaultValueOnEmptyText="True"/>
                                                    </DockPanel>
                                                </Border>
                                            </Grid>
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="*" />
                                                    <ColumnDefinition Width="Auto" />
                                                </Grid.ColumnDefinitions>
                                                <Label x:Name="lblNPCReplacement" Content="Replacement:" Grid.Column="0"/>
                                                <Border Grid.Column="2" Margin="0,3,4,3">
