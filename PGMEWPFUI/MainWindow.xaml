                                                        <TextBox DockPanel.Dock="Left" BorderThickness="1,1,0,1" Text="{Binding Source={x:Static bknd:Config.settings}, Path=HexPrefix}" IsReadOnly="True" IsTabStop="False" MaxLength="2" Background="{DynamicResource {x:Static SystemColors.InactiveSelectionHighlightBrushKey}}"/>
                                                        <TextBox x:Name="tbNPCReplacement" FontFamily="Courier New" MinWidth="20" DockPanel.Dock="Right" BorderThickness="0,1,1,1" Text="00" MaxLength="2" PreviewTextInput="HexBoxOnPreviewTextInput" CharacterCasing="Upper" Padding="0,3,0,0"/>
                                                    </DockPanel>
                                                </Border>
                                            </Grid>
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="*" />
                                                    <ColumnDefinition Width="Auto" />
                                                </Grid.ColumnDefinitions>
                                                <Label x:Name="lblNPCFiller1" Content="Filler:" Grid.Column="0"/>
                                                <Border Grid.Column="2" Margin="0,3,4,3">
                                                    <DockPanel>
                                                        <TextBox DockPanel.Dock="Left" BorderThickness="1,1,0,1" Text="{Binding Source={x:Static bknd:Config.settings}, Path=HexPrefix}" IsReadOnly="True" IsTabStop="False" MaxLength="2" Background="{DynamicResource {x:Static SystemColors.InactiveSelectionHighlightBrushKey}}"/>
                                                        <TextBox x:Name="tbNPCFiller1" FontFamily="Courier New" Padding="0,3,0,0" MinWidth="20" DockPanel.Dock="Right" BorderThickness="0,1,1,1" Text="00" MaxLength="2" PreviewTextInput="HexBoxOnPreviewTextInput" CharacterCasing="Upper"/>
                                                    </DockPanel>
                                                </Border>
                                            </Grid>
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="*" />
                                                    <ColumnDefinition Width="Auto" />
                                                </Grid.ColumnDefinitions>
                                                <Label x:Name="lblNPCPosition" Content="Position:" Canvas.Top="104"/>
                                                <StackPanel Orientation="Horizontal" Grid.Column="2">
                                                    <Border Grid.Column="2" Margin="0,3,4,3">
                                                        <DockPanel>
                                                            <TextBox DockPanel.Dock="Left" BorderThickness="1,1,0,1" Text="{Binding Source={x:Static bknd:Config.settings}, Path=HexPrefix}" IsReadOnly="True" IsTabStop="False" MaxLength="2" Background="{DynamicResource {x:Static SystemColors.InactiveSelectionHighlightBrushKey}}"/>
                                                            <TextBox x:Name="tbNPCXPos" FontFamily="Courier New" Padding="0,3,0,0" MinWidth="34" DockPanel.Dock="Right" BorderThickness="0,1,1,1" Text="0000" MaxLength="4" PreviewTextInput="HexBoxOnPreviewTextInput" CharacterCasing="Upper"/>
                                                        </DockPanel>
                                                    </Border>
                                                    <Border Grid.Column="2" Margin="0,3,4,3">
                                                        <DockPanel>
                                                            <TextBox DockPanel.Dock="Left" BorderThickness="1,1,0,1" Text="{Binding Source={x:Static bknd:Config.settings}, Path=HexPrefix}" IsReadOnly="True" IsTabStop="False" MaxLength="2" Background="{DynamicResource {x:Static SystemColors.InactiveSelectionHighlightBrushKey}}"/>
                                                            <TextBox x:Name="tbNPCYPos" FontFamily="Courier New" Padding="0,3,0,0" MinWidth="34" DockPanel.Dock="Right" BorderThickness="0,1,1,1" Text="0000" MaxLength="4" PreviewTextInput="HexBoxOnPreviewTextInput" CharacterCasing="Upper"/>
                                                        </DockPanel>
                                                    </Border>
                                                </StackPanel>
                                            </Grid>
                                            <Label x:Name="lblNPCHeight" Content="Height:"/>
                                            <Border Padding="0,3,4,3">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="Auto" />
                                                        <ColumnDefinition Width="*" />
                                                    </Grid.ColumnDefinitions>
                                                    <Border Grid.Column="0" Margin="6,1,4,1">
                                                        <DockPanel>
                                                            <TextBox DockPanel.Dock="Left" BorderThickness="1,1,0,1" Text="{Binding Source={x:Static bknd:Config.settings}, Path=HexPrefix}" IsReadOnly="True" IsTabStop="False" MaxLength="2" Background="{DynamicResource {x:Static SystemColors.InactiveSelectionHighlightBrushKey}}"/>
                                                            <TextBox x:Name="tbNPCHeight" FontFamily="Courier New" Padding="0,3,0,0" MinWidth="20" DockPanel.Dock="Right" BorderThickness="0,1,1,1" Text="00" MaxLength="2" PreviewTextInput="HexBoxOnPreviewTextInput" CharacterCasing="Upper"/>
                                                        </DockPanel>
                                                    </Border>
                                                    <ComboBox Grid.Column="1" x:Name="cbNPCHeight"/>
                                                </Grid>
                                            </Border>
                                            <Label x:Name="lblNPCIdleAnim" Content="Idle Animation:"/>
                                            <Border Padding="0,3,4,3">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="Auto" />
                                                        <ColumnDefinition Width="*" />
                                                    </Grid.ColumnDefinitions>
                                                    <Border Grid.Column="0" Margin="6,1,4,1">
                                                        <DockPanel>
                                                            <TextBox DockPanel.Dock="Left" BorderThickness="1,1,0,1" Text="{Binding Source={x:Static bknd:Config.settings}, Path=HexPrefix}" IsReadOnly="True" IsTabStop="False" MaxLength="2" Background="{DynamicResource {x:Static SystemColors.InactiveSelectionHighlightBrushKey}}"/>
