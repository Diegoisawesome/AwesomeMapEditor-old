                                                        </DockPanel>
                                                    </Border>
                                                    <Border Margin="0,3,4,3">
                                                        <DockPanel>
                                                            <TextBox DockPanel.Dock="Left" BorderThickness="1,1,0,1" Text="{Binding Source={x:Static bknd:Config.settings}, Path=HexPrefix}" IsReadOnly="True" IsTabStop="False" MaxLength="2" Background="{DynamicResource {x:Static SystemColors.InactiveSelectionHighlightBrushKey}}"/>
                                                            <TextBox x:Name="tbNPCYBounds" FontFamily="Courier New" Padding="0,3,0,0" MinWidth="12" DockPanel.Dock="Right" BorderThickness="0,1,1,1" Text="0" MaxLength="1" PreviewTextInput="HexBoxOnPreviewTextInput" CharacterCasing="Upper"/>
                                                        </DockPanel>
                                                    </Border>
                                                </StackPanel>
                                            </Grid>
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="*" />
