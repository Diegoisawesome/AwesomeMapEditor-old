                                                        <TextBox x:Name="tbNPCFiller2" FontFamily="Courier New" Padding="0,3,0,0" MinWidth="20" DockPanel.Dock="Right" BorderThickness="0,1,1,1" Text="00" MaxLength="2" PreviewTextInput="HexBoxOnPreviewTextInput" CharacterCasing="Upper"/>
